@model MAS_TestiranjeSoftvera_Projekat.Domain.Mesto

@{
    ViewData["Title"] = "Delete";
}


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Delete">

            <input type="hidden" asp-for="MestoId" class="form-control" />

            <div class="form-group">
                <label asp-for="Naziv" class="control-label"></label>
                <input asp-for="Naziv" class="form-control" readonly="readonly" />
            </div>
            <div class="form-group">
                <label asp-for="PttBroj" class="control-label"></label>
                <input asp-for="PttBroj" class="form-control" readonly="readonly" />
            </div>
            <div class="form-group">
                <label asp-for="BrojStanovnika" class="control-label"></label>
                <input asp-for="BrojStanovnika" class="form-control" readonly="readonly"/>
            </div>

            <div class="form-group">
                <input type="submit" value="Obrisi" class="btn btn-danger" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}



